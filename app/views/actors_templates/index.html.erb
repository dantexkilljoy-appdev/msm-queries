<h1>
List of all actors
</h1>

<table border="1">
<% @list_of_actors.each do |actor| %>
  <tr>
    <td>
    <%= actor.id %>
    </td>

    <td>
    <img src="https://robohash.org/<%= actor.image %>?set=set4">
    </td>

    <td>
    <%= actor.name %>
    </td>

    <td>
    <%= actor.dob %>
    </td>

    <td>
      <a href="/actors/<%= actor.id %>">
      Show Details</a>
    </td>

  </tr>

  <% end %>

</table>
